        <Confianz_Taxes><!-- COMPANYNAME_MODULE -->
            <version>0.0.0.1</version>
        </Confianz_Taxes>
    </modules>
    <global>
        <models>
            <taxes><!-- MODULE, just first letter is lowercased, i.e. module CustomAPI would be placed here as customAPI -->
